{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js",
      "headers": [
        {
          "source": "Access-Control-Allow-Origin",
          "value": "https://avant-fullstack.vercel.app"
        }
      ]
    }
  ]
}
